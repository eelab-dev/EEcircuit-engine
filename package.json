    "test:source": "npx tsx test/test.ts",
    "test:package": "npx tsx test/test-package.ts"
  },
  "author": "Danial Chitnis",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4"
  }
}
